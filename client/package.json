{
    "name": "weatherman",
    "version": "0.0.1",
    "dependencies": {
        "@material-ui/core": "^1.3.0",
        "axios": "^0.18.0",
        "babel-core": "6.26.0",
        "babel-eslint": "7.2.3",
        "babel-jest": "20.0.3",
        "babel-loader": "7.1.2",
        "babel-preset-react-app": "^3.1.1",
        "babel-runtime": "6.26.0",
        "css-loader": "0.28.7",
        "eslint": "4.10.0",
        "eslint-config-react-app": "^2.1.0",
        "eslint-loader": "1.9.0",
        "eslint-plugin-flowtype": "2.39.1",
        "eslint-plugin-import": "2.8.0",
        "eslint-plugin-jsx-a11y": "5.1.1",
        "eslint-plugin-react": "7.4.0",
        "highcharts": "^6.1.1",
        "highcharts-react-official": "^1.2.0",
        "html-webpack-plugin": "2.29.0",
        "install": "^0.10.4",
        "jest": "20.0.4",
        "moment": "^2.22.1",
        "moment-timezone": "^0.5.17",
        "node-sass-chokidar": "0.0.3",
        "npm": "^5.6.0",
        "npm-run-all": "^4.0.2",
        "react": "^16.2.0",
        "react-dev-utils": "^5.0.0",
        "react-dom": "^16.2.0",
        "react-icons": "^2.2.7",
        "react-intl-universal": "^1.11.1",
        "react-moment": "^0.7.0",
        "react-pull-to-refresh": "^1.1.2",
        "react-redux": "^5.0.6",
        "react-responsive": "^4.0.4",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^5.0.0-alpha.9",
        "react-scripts": "^1.0.10",
        "react-select": "^1.2.1",
        "redux": "^3.7.2",
        "redux-mock-store": "^1.5.1",
        "redux-thunk": "^2.2.0",
        "style-loader": "0.19.0",
        "toastr": "^2.1.4",
        "typeface-roboto": "0.0.54",
        "webpack": "3.8.1",
        "webpack-dev-server": "2.9.4",
        "webpack-manifest-plugin": "1.3.2",
        "whatwg-fetch": "2.0.3"
    },
    "engines": {
        "node": ">=8.8"
    },
    "files": [
        "build",
        "gradle",
        "deploy.gradle",
        "build.gradle",
        "gradlew",
        "config",
        "scripts"
    ],
    "scripts": {
        "install:dev": "npm install",
        "start": "cross-env NODE_PATH=src  npm-run-all -p watch-css start-js",
        "test": "cross-env NODE_PATH=src jest --coverage --env=jsdom",
        "build": "npm run build-css && NODE_PATH=src react-scripts build",
        "build-css": "node-sass-chokidar src/App.scss -o src/",
        "watch-css": "npm run build-css && node-sass-chokidar src/App.scss -o src/ --watch --recursive",
        "start-js": "react-scripts start",
        "updateSnapshot": "jest --updateSnapshot",
        "format": "prettier --write --print-width 120 --tab-width 4 --single-quote --parser babylon '**/*.js'"
    },
    "devDependencies": {
        "cross-env": "^5.1.3",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "enzyme-react-intl": "^1.4.8",
        "enzyme-to-json": "^3.3.1",
        "jest-html-reporter": "^2.0.0",
        "lint-staged": "^4.0.2",
        "node-sass": "^4.7.2",
        "react-test-renderer": "^16.2.0",
        "redux-logger": "^3.0.6",
        "sass-loader": "^6.0.6"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,mjs}"
        ],
        "setupFiles": [
            "<rootDir>/config/polyfills.js"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
            "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
        ],
        "testEnvironment": "jsdom",
        "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web"
        },
        "moduleFileExtensions": [
            "web.js",
            "mjs",
            "js",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ]
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    },
    "lint-staged": {
        "*.{js,json,scss}": [
            "prettier --write --print-width 120 --tab-width 4 --single-quote --parser babylon",
            "git add"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    }
}
